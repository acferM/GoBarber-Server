    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
